{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Skill</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Skillpath</th>
                <td>{{ entity.skillPath }}</td>
            </tr>
            <tr>
                <th>Skillcategory</th>
                <td>{{ entity.skillCategory }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('skill') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('skill_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
